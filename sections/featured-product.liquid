{%- liquid
  assign section_color = section.settings.color_scheme
  assign enable_video_looping = section.settings.enable_video_looping

  case section.settings.desktop_media_width
    when 'one-third'
      assign content_width_desktop = 'lg:col-span-8'

      assign media_sizes = '(min-width: 1024px) calc(100vw / 3), 100vw'

      if section.settings.desktop_media_position == 'right'
        assign media_width_desktop = 'lg:col-start-9 lg:col-end-13'
      else
        assign media_width_desktop = 'lg:col-span-4'
      endif
    when 'one-half'
      assign content_width_desktop = 'lg:col-span-6'

      assign media_sizes = '(min-width: 1024px) 50vw, 100vw'

      if section.settings.desktop_media_position == 'right'
        assign media_width_desktop = 'lg:col-start-7 lg:col-end-13'
      else
        assign media_width_desktop = 'lg:col-span-6'
      endif
    when 'three-fifths'
      assign content_width_desktop = 'lg:col-span-5'

      assign media_sizes = '(min-width: 1024px) calc(100vw / 5 * 3), 100vw'

      if section.settings.desktop_media_position == 'right'
        assign media_width_desktop = 'lg:col-start-6 lg:col-end-13'
      else
        assign media_width_desktop = 'lg:col-span-7'
      endif
    when 'two-thirds'
      assign content_width_desktop = 'lg:col-span-4'

      assign media_sizes = '(min-width: 1024px) calc(100vw / 3 * 2), 100vw'

      if section.settings.desktop_media_position == 'right'
        assign media_width_desktop = 'lg:col-start-5 lg:col-end-13'
      else
        assign media_width_desktop = 'lg:col-span-8'
      endif
  endcase

  assign product = all_products[section.settings.product]
  assign current_variant = product.selected_or_first_available_variant

  if section.settings.always_show_product_featured_media_first
    assign featured_media = product.featured_media
  else
    assign featured_media = current_variant.featured_media | default: product.featured_media
  endif

  assign complementary_products_block = section.blocks | where: 'type', 'complementary' | first

  assign preload = false
  assign loading = 'lazy'

  if section.index == 1
    assign preload = true
  endif

  if section.index <= 3
    assign loading = 'eager'
  endif
-%}

{{ 'modules-product.bundle.css' | asset_url | stylesheet_tag }}

{% if product != blank %}
  <section
    class="grid grid-flow-row-dense grid-cols-1 border-b-gridline border-gridline-color lg:grid-cols-12 lg:gap-gutter lg:bg-gridline-color"
    x-data="
      Theme_Product({
        product: {{ product | json | escape }},
        initialVariant: {{ current_variant | json | escape }},
        optionsWithValues: {{ product.options_with_values | json | escape }},
        featuredMediaId: {% if featured_media %}{{ featured_media.id }}{% else %}null{% endif %},
        shouldUpdateHistoryState: false,
      })
    "
    data-product-root="{{ section.id }}"
    data-enable-image-zoom="false"
    data-color-scheme="{{ section_color }}"
  >
    <div
      class="flex {{ section.settings.desktop_content_position }} product-media-container relative bg-scheme-background text-scheme-text {{ media_width_desktop }} {% unless settings.fill_image_to_border_edge %}border-b-gridline border-gridline-color lg:border-0{% endunless %}"
      data-product-media-container
    >
      <div class="w-full">
        <div class="{% unless settings.fill_image_to_border_edge %}lg:p-4{% endunless %}">
          <ul>
            <li x-show="currentMediaId == {{ featured_media.id }}">
              {% render 'product-media',
                media: featured_media,
                sizes: media_sizes,
                enable_video_looping: enable_video_looping,
                preload: preload,
                loading: loading
              %}
            </li>
            {% for media in product.media %}
              {% unless media.id == featured_media.id %}
                <li
                  x-show="currentMediaId == {{ media.id }}"
                  style="display: none;"
                >
                  {% render 'product-media', media: media, sizes: media_sizes %}
                </li>
              {% endunless %}
            {% endfor %}
          </ul>
        </div>
        {% assign first_3d_model = product.media
          | where: 'media_type', 'model'
          | first
        %}
        {%- if first_3d_model -%}
          <div class="px-section-horizontal-spacing text-center">
            <button
              aria-label="{{ 'products.product.view_in_space_label' | t }}"
              class="product-single__view-in-space mb-2 mt-4 w-full max-w-sm border-text border-scheme-text py-2 pl-2 pr-4 text-center lg:my-4"
              data-shopify-xr
              data-shopify-model3d-id="{{ first_3d_model.id }}"
              data-shopify-title="{{ product.title }}"
              data-shopify-xr-hidden
            >
              <span class="inline-block w-6 align-middle">
                {%- render 'icon-3d-model' -%}</span
              ><span class="text-sm">
                {{- 'products.product.view_in_space' | t -}}
              </span>
            </button>
          </div>
        {%- endif -%}
      </div>
    </div>

    <div
      class="flex {{ section.settings.desktop_content_position }} product-content-container bg-scheme-background px-section-horizontal-spacing text-scheme-text {{ content_width_desktop }}"
    >
      <div class="product-block-area w-full py-section-vertical-spacing {{ section.settings.max_width_of_content }}">
        {%- render 'product-form',
          product: product,
          current_variant: current_variant,
          section_color: section_color,
          featured_product_section: true
        -%}
      </div>
    </div>

    {% unless product == empty %}
      <script type="application/json" id="ModelJson-{{ section.id }}">
        {{ product.media | where: 'media_type', 'model' | json }}
      </script>
    {% endunless %}
  </section>

  {% unless request.design_mode %}
    <script
      src="{{ 'splide.bundle.min.js' | asset_url }}"
      type="module"
    ></script>
    <script src="{{ 'product-form.js' | asset_url }}" type="module"></script>
    <script src="{{ 'product.js' | asset_url }}" type="module"></script>
  {% endunless %}

  <script type="application/ld+json">
    {
      "@context": "http://schema.org/",
      "@type": "Product",
      "name": {{ product.title | json }},
      "url": {{ shop.url | append: product.url | json }},
      {%- if product.featured_media -%}
        {%- assign media_size = product.featured_media.preview_image.width | append: 'x' -%}
        "image": [
          {{ product.featured_media | image_url: media_size | prepend: "https:" | json }}
        ],
      {%- endif -%}
      "description": {{ product.description | strip_html | json }},
      {%- if current_variant.sku != blank -%}
        "sku": {{ current_variant.sku | json }},
      {%- endif -%}
      "brand": {
        "@type": "Thing",
        "name": {{ product.vendor | json }}
      },
      "offers": [
        {%- for variant in product.variants -%}
          {
            "@type" : "Offer",
            {%- if variant.sku != blank -%}
              "sku": {{ variant.sku | json }},
            {%- endif -%}
            "availability" : "http://schema.org/{% if variant.available %}InStock{% else %}OutOfStock{% endif %}",
            "price" : {{ variant.price | divided_by: 100.00 | json }},
            "priceCurrency" : {{ cart.currency.iso_code | json }},
            "url" : {{ shop.url | append: variant.url | json }}
          }{% unless forloop.last %},{% endunless %}
        {%- endfor -%}
      ]
    }
  </script>
{% else %}
  <section
    class="grid grid-flow-row-dense grid-cols-1 border-b-gridline border-gridline-color lg:grid-cols-12 lg:gap-gutter lg:bg-gridline-color"
    data-color-scheme="{{ section_color }}"
  >
    <div
      class="flex {{ section.settings.desktop_content_position }} product-media-container relative bg-scheme-background text-scheme-text {{ media_width_desktop }} {% unless settings.fill_image_to_border_edge %}border-b-gridline border-gridline-color lg:border-0{% endunless %}"
      data-product-media-container
    >
      <div class="w-full">
        <div class="{% unless settings.fill_image_to_border_edge %}lg:p-4{% endunless %}">
          <ul class="{% if settings.fill_image_to_border_edge %}border-b-gridline border-gridline-color lg:border-0{% endif %} lg:grid lg:w-full lg:grid-cols-1 {% if settings.fill_image_to_border_edge %}lg:gap-gutter lg:bg-gridline-color{% else %}lg:gap-4{% endif%}">
            <li class="{% unless settings.fill_image_to_border_edge %}p-2 lg:p-0{% else %}w-full bg-scheme-background{% endunless %}">
              <div>
                {{
                  'product-1'
                  | placeholder_svg_tag: 'text-scheme-placeholder-foreground bg-scheme-placeholder-background fill-current'
                }}
              </div>
            </li>
          </ul>
        </div>
      </div>
    </div>

    <div class="flex {{ section.settings.desktop_content_position }} product-content-container bg-scheme-background px-section-horizontal-spacing text-scheme-text {{ content_width_desktop }}">
      <div class="py-section-vertical-spacing">
        <h2 class="font-heading text-heading-feature">
          {{ 'homepage.onboarding.product_title' | t }}
        </h2>

        <div class="mt-2 text-xl">
          <span>
            {%- if settings.currency_code_enabled == true -%}
              {{- 1999 | money_with_currency -}}
            {%- else -%}
              {{- 1999 | money -}}
            {%- endif -%}
          </span>
        </div>

        <div class="add-to-cart-container mt-8 lg:w-3/4">
          <button
            class="add-to-cart theme-button w-fit cursor-not-allowed opacity-50 after:content-none"
            type="submit"
            disabled="disabled"
          >
            {{ 'products.product.unavailable' | t }}
          </button>
        </div>
      </div>
    </div>
  </section>
{% endif %}

{%- liquid
  if complementary_products_block
    case desktop_media_width
      when 'one-third'
        assign desktop_content_column_sizes_base = 'calc(100vw / 3)'
      when 'one-half'
        assign desktop_content_column_sizes_base = '50vw'
      when 'three-fifths'
        assign desktop_content_column_sizes_base = '40vw'
      when 'two-thirds'
        assign desktop_content_column_sizes_base = 'calc((100vw / 3 * 2))'
    endcase
    # theme-check-disable NestedSnippet
    render 'product-block-complementary-products-output', complementary_products_block: complementary_products_block, desktop_sizes_base: desktop_content_column_sizes_base
    # theme-check-enable NestedSnippet
  endif
-%}

